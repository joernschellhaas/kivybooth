#:kivy 2.0.0

LoginScreen:
    name: 'Login'
    keyboard: True

    AdaptiveBoxLayout:
        orientation: 'vertical'
            
        TextInput:
            id: password
            password: True
            #unfocus_on_touch: False
            
        KBButton:
            text: 'Login'
            on_release: app.go_to_screen("admin", Direction.REPLACE)  
